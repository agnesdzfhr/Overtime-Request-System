@{
    ViewData["Title"] = "Employee Request";
    Layout = "_AdminLayout";
}

<section class="section">
    <div class="section-header">
        <h1>Employee Request</h1>
    </div>

    <div class="section-body">
        <div class="row">
            <div class="col-12">

                <div class="card">
                    <div class="card-header">
                        <h4>Request Data</h4>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped" id="tableValidateBonus">
                                <thead>
                                    <tr>
                                        <th class="text-center">
                                            No
                                        </th>
                                        <th>NIK</th>
                                        <th>Name</th>
                                        <th>Date</th>
                                        <th>Start-Time</th>
                                        <th>End-Time</th>
                                        <th>Salary</th>
                                        <th>TotalFee</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <script src="~/js/Finance.js" asp-append-version="true"></script>
}


