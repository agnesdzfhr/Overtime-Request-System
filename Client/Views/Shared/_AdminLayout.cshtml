                            <a href="#" class="nav-link has-dropdown"><i class="far fa-file-alt"></i><span>Menu</span></a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a asp-area="" asp-controller="RequestForm" asp-action="Index" class="nav-link"><span>Request Form</span></a>
                                </li>
                                @if ((User.IsInRole("Manager"))|| (User.IsInRole("Admin")))
                                {
                                    <li>
                                        <a asp-area="" asp-controller="EmployeeRequest" asp-action="Index" class="nav-link"><span>Employee Request (M)</span></a>
                                    </li>
                                }
                                @if ((User.IsInRole("Finance")) || (User.IsInRole("Admin")))
                                {
                                    <li>
                                        <a asp-area="" asp-controller="EmployeeRequest" asp-action="Index" class="nav-link"><span>Employee Request (F)</span></a>
                                    </li>
                                }
                                <li>
                                    <a asp-area="" asp-controller="Home" asp-action="AcceptRequest" class="nav-link"><span>Overtime Request History</span></a>
                                </li>
                            </ul>
                        </li>
                        @if (User.IsInRole("Admin"))
                        {
                            <li class="menu-header">Manage APP</li>
                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="ManageUser" class="nav-link"><i class="far fa-user"></i><span>Manage User</span></a>
                            </li>
                        }


                        </ul>
